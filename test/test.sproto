.PhoneNumber {
    number 99 : string
    type  1000: integer
}

.IntKeyStringValue {
    key 0: integer
    value 1: string
}

.Person {
	name 0 : string
	id 1 : integer


	phone 4 : *PhoneNumber
	phonemap 5 : *PhoneNumber()
    phonemapkv 6 : *PhoneNumber(number)
	onephone 7 : PhoneNumber

    i2s 8 : *IntKeyStringValue()
}

.AddressBook {
	person 0: *Person(id)
}
